<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="Cuyahoga.Modules.ECommerce" namespace="Cuyahoga.Modules.ECommerce.Domain">
  <class name="Country" table="Ecommerce_Country" lazy="true">

		<id name="_CountryCode" column="countryCode" type="String">
			<generator class="assigned"/>
		</id>
		<property column="countryName" type="String" name="_CountryName" not-null="true" length="128" />
		<many-to-one name="_DefaultCurrencyCode" column="defaultCurrencyCode" class="CurrencyInfo" />
		<property column="inserttimestamp" type="DateTime" name="_Inserttimestamp" not-null="true" />
		<property column="updatetimestamp" type="DateTime" name="_Updatetimestamp" not-null="true" />
		
	</class>
</hibernate-mapping>
