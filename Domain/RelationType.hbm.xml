<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="Cuyahoga.Modules.ECommerce" namespace="Cuyahoga.Modules.ECommerce.Domain">

  <class name="RelationType" table="Ecommerce_RelationType" lazy="true">

		<id name="_RelationTypeid" column="relationTypeID" type="Int16" unsaved-value="0">
			<generator class="assigned"/>
		</id>
		<property column="relationshipName" type="String" name="_RelationshipName" not-null="true" length="128" />
		<property column="relationshipDescription" type="String" name="_RelationshipDescription" length="1024" />
		
	</class>
</hibernate-mapping>
