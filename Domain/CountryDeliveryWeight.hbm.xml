<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="Cuyahoga.Modules.ECommerce" namespace="Cuyahoga.Modules.ECommerce.Domain">

  <class name="CountryDeliveryWeight" table="Ecommerce_CountryDeliveryWeight" lazy="true">

    <composite-id access="field">
      <key-property name="CountryCode" column="countryCode" type="String" />
      <key-property name="WeightLevel" column="weightLevel" type="Decimal" />

    </composite-id>

    <property column="price" type="Decimal" name="Price" not-null="true" />



  </class>
</hibernate-mapping>
