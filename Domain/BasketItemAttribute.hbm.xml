  <class name="BasketItemAttribute" table="Ecommerce_BasketItemAttribute" lazy="true">

		<id name="_Basketitemid" column="basketitemID" type="Int64" unsaved-value="0">
			<generator class="assigned"/>
		</id>
		<property column="attributeID" type="Int32" name="_Attributeid" not-null="true" />
		<property column="optionValue" type="String" name="_OptionValue" not-null="true" length="10" />
		<property column="optionPrice" type="Int64" name="_OptionPrice" not-null="true" />
		<property column="inserttimestamp" type="DateTime" name="_Inserttimestamp" not-null="true" />
		<property column="updatetimestamp" type="DateTime" name="_Updatetimestamp" not-null="true" />
  </class>
</hibernate-mapping>
